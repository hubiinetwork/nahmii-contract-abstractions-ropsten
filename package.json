{
  "name": "nahmii-contract-abstractions-ropsten",
  "version": "1.1.0-rc2",
  "description": "Smart contract abstractions for nahmii deployed to Ropsten",
  "keywords": [
    "nahmii",
    "smart contracts",
    "ethereum"
  ],
  "license": "LGPL-3.0",
  "author": {
    "name": "Jens Ivar JÃ¸rdre",
    "email": "jensivar@hubii.com"
  },
  "main": "lib/index.js",
  "repository": "github:hubiinetwork/nahmii-contract-abstractions-ropsten",
  "scripts": {
    "publish:major": "npm version major && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "symlink:build": "[ ! -d ../../build/contracts ] && mkdir -p ../../build && ln -vs $(pwd)/build/contracts ../../build/contracts",
    "symlink:build:force": "rm -rf ../../build/contracts && npm run symlink:build",
    "symlink:events": "[ ! -d ../../events ] && ln -vs $(pwd)/events ../../events",
    "symlink:events:force": "rm -rf ../../events && npm run symlink:events",
    "test": "mocha lib/**/*.spec.js"
  },
  "dependencies": {
    "contract-abstractions-manager": "github:hubiinetwork/contract-abstractions-manager"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "glob": "^7.1.3",
    "mocha": "^6.0.2"
  }
}
